
public class BinaryOperator {

	public static void main(String[] args) {
		
		//산술연산(+,-,*,/,%)
		
		int i = 7/3; //몫을 구한다.
		int j = 7%3; //나머지를 구한다. 
		
		System.out.println(i);
		System.out.println(j);
		
		//비교 연산자(<,<=,>,>=,==,!=)
		//결과값은 논리값이다. 
		System.out.println(i == j);
		System.out.println(20 >= 20);
		System.out.println(130 < 50);
		
		
		
	}

}
