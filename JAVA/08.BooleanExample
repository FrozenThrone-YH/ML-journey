
public class BooleanExample {

		public static void main(String[] args) {
			
			boolean b1 = true;
			boolean b2 = false;
			// boolean 변수는 딱 두개만 받을 수 있다.
			// python 처럼 False를 치면 안된다.
			
			boolean b4 = "false";
			//이거는 문장 false지 불리언이 아니다.
			
			boolean b5 = 0;
			// 0은 정수형이다. 논리형이 아니다
			
		}
	
}
