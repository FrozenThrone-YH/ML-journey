#미완성 

cost = list(map(int,input().split(';')))
cost.sort(reverse=True)

for i in cost:
    c = list(str(i))
    c.reverse()
    for j in range(1,len(c)):
        if j % 4 == 0:
            c.insert(j-1,',')
    c.reverse().join()
    print(c)
    
